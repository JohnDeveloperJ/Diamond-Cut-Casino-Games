					"details": "event emitted by the VRF callback with the bet results",
					"params": {
						"diceOutcomes": "results of dice roll, range 0-9999",
						"numGames": "number of games performed",
						"payout": "total payout transfered to the player",
						"payouts": "individual payouts for each bet",
						"playerAddress": "address of the player that made the bet",
						"tokenAddress": "address of token the wager was made and payout, 0 address is considered the native coin",
						"wager": "wager amount"
					}
				},
				"Dice_Play_Event(address,uint256,uint32,address,bool,uint32,uint256,uint256,uint256)": {
					"details": "event emitted at the start of the game",
					"params": {
						"isOver": "if true dice outcome must be over the selected number, false must be under",
						"multiplier": "selected multiplier for the wager range 10421-9900000, multiplier values divide by 10000",
						"numBets": "number of bets the player intends to make",
						"playerAddress": "address of the player that made the bet",
						"stopGain": "gain value at which the betting stop if a gain is reached",
						"stopLoss": "loss value at which the betting stop if a loss is reached",
						"tokenAddress": "address of token the wager was made, 0 address is considered the native coin",
						"wager": "wagered amount"
					}
				},
				"Dice_Refund_Event(address,uint256,address)": {
					"details": "event emitted when a refund is done in dice",
					"params": {
						"player": "address of the player reciving the refund",
						"tokenAddress": "address of token the refund was made in",
						"wager": "amount of wager that was refunded"
					}
				}
			},
			"kind": "dev",
			"methods": {
				"Dice_GetState(address)": {
					"details": "function to get current request player is await from VRF, returns 0 if none",
					"params": {
						"player": "address of the player to get the state"
					}
				},
				"Dice_Play(uint256,uint32,address,bool,uint32,uint256,uint256)": {
					"details": "Function to play Dice, takes the user wager saves bet parameters and makes a request to the VRF",
					"params": {
						"isOver": "if true dice outcome must be over the selected number, false must be under",
						"multiplier": "selected multiplier for the wager range 10421-9900000, multiplier values divide by 10000",
						"numBets": "number of bets to make, and amount of random numbers to request",
						"stopGain": "treshold value at which the bets stop if a certain profit is obtained",
						"stopLoss": "treshold value at which the bets stop if a certain loss is obtained",
						"tokenAddress": "address of token to bet, 0 address is considered the native coin",
						"wager": "wager amount"
					}
				},
				"Dice_Refund()": {
					"details": "Function to refund user in case of VRF request failling"
				},
				"getVRFFee(uint256)": {
					"details": "calculates in form of native token the fee charged by chainlink VRF",
					"returns": {
						"fee": "amount of fee user has to pay"
					}
				},
				"rawFulfillRandomWords(uint256,uint256[])": {
					"details": "function called by Chainlink VRF with random numbers",
					"params": {
						"randomWords": "array of random numbers",
						"requestId": "id provided when the request was made"
					}
				},
				"transferFees(address)": {
					"details": "function to transfer VRF fees acumulated in the contract to the Bankroll Can only be called by owner"
				}
